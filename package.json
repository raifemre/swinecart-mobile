{
  "name": "SwineCart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "react-native link",
    "test-android": "detox build --configuration android.emu.debug && detox test --configuration android.emu.debug -l verbose"
  },
  "dependencies": {
    "axios": "0.18.0",
    "buffer": "5.2.1",
    "lodash": "4.17.11",
    "mobx": "5.5.0",
    "mobx-react": "5.2.8",
    "moment": "2.22.2",
    "native-base": "2.8.0",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-divider": "1.0.3",
    "react-native-fast-image": "5.0.10",
    "react-native-gifted-chat": "0.4.3",
    "react-native-snap-carousel": "3.7.5",
    "react-native-spinkit": "1.1.1",
    "react-native-typography": "1.3.0",
    "react-native-uuid": "1.4.9",
    "react-navigation": "2.14.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-decorators": "7.1.0",
    "babel-jest": "23.6.0",
    "detox": "9.0.4",
    "jest": "23.6.0",
    "jsc-android": "225067.0.0",
    "metro-react-native-babel-preset": "0.45.2",
    "mocha": "5.2.0",
    "react-native-dotenv": "0.2.0",
    "react-test-renderer": "16.5.0"
  },
  "jest": {
    "preset": "react-native",
    "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_23"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_23"
      }
    }
  }
}
